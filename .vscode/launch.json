	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "test:file",
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ava",
			"runtimeArgs": ["debug", "${file}"],
			"console": "integratedTerminal",
			"port": 9229,
			"skipFiles": ["<node_internals>/**/*.js"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "test:all",
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ava",
			"runtimeArgs": ["debug"],
			"console": "integratedTerminal",
			"port": 9229,
			"skipFiles": ["<node_internals>/**/*.js"]
		}
	]
}
